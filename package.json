{
	"name": "oterlu-api",
	"version": "0.0.5",
	"description": "An unofficial API wrapper for Oterlu.",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"author": "Axis (axis@axis.lol)",
	"license": "MIT",
	"scripts": {
		"build": "run-s transpile types esm",
		"transpile": "tsc",
		"types": "tsc --declaration --emitDeclarationOnly",
		"esm": "gen-esm-wrapper ./dist/index.js ./dist/index.mjs",
		"sversion": "standard-version",
		"commit": "git-cz",
		"cz": "git-cz",
		"prepare": "husky install .github/husky"
	},
	"dependencies": {
		"petitio": "^1.4.0",
		"tslib": "^2.3.1"
	},
	"devDependencies": {
		"@commitlint/cli": "^16.1.0",
		"@commitlint/config-conventional": "^16.0.0",
		"@favware/npm-deprecate": "^1.0.4",
		"@types/node": "^17.0.13",
		"cz-conventional-changelog": "^3.3.0",
		"gen-esm-wrapper": "^1.1.3",
		"husky": "^7.0.4",
		"lint-staged": "^12.3.2",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.5.1",
		"standard-version": "^9.3.2",
		"typescript": "^4.5.5"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/axisiscool/oterlu-api.git"
	},
	"files": [
		"dist"
	],
	"engines": {
		"node": ">=15.0.0",
		"npm": ">=7.0.0"
	},
	"keywords": [
		"oterlu",
		"oterlu-api",
		"oterlu api"
	],
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.ts": "prettier --write ./src/**/*.ts"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"packageManager": "yarn@3.1.1"
}
